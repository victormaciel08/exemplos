#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int maior(int num1, int num2);

int main(void) {
	
	int m;
	int x;
	int y;
	
	printf("Digite um numero para X: ");
	scanf(" %d", &x);
	
	printf("Digite um numero para Y: ");
	scanf(" %d", &y);
	
	m = maior(x, y);
	
	printf("\nMaior : %d", m);
	
    return 0;
}

int maior(int num1, int num2){
	
	if (num1 > num2){
		return num1;
	}
	else{
		return num2;
	}
}
