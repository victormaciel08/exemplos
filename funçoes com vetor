#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void teste1(int vetor[], int n) {
    int i;
    for (i = 0; i < n; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");
}

void teste2(int vetor[8]) {
    int i;
    for (i = 0; i < 8; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");
}

void teste3(int vetor[], int n) {
    int i;
    for (i = 0; i < n; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");
}

int main(void) {
    int vetor1[5] = {1, 3, 5, 7, 9};
    int vetor2[8] = {2, 4, 6, 8, 10, 12, 14, 16}; 
    int vetor3[5] = {1, 2, 3, 4, 5};

    teste1(vetor1, 5);
    
    teste2(vetor2); 
    
    teste3(vetor3, 5);

    return 0;
}
